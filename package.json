{
    "name": "phpfixer",
    "displayName": "PHPFixer",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.79.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "PHPFixer",
            "properties": {
                "PHPFixer.apiUrl": {
                    "type": "string",
                    "default": "https://9a47-104-199-198-216.ngrok.io/",
                    "description": "API URL for generate candidate patches"
                },
                "PHPFixer.numOfCandidatePatches": {
                    "type": "number",
                    "default": 5,
                    "description": "Number of Candidate patches"
                }
            }
        },
        "commands": [
            {
                "command": "phpfixer.fix-php",
                "title": "Fix Line",
                "category": "PHP Fixer"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "phpfixer.fix-php",
                    "when": "resourceLangId == php && editorTextFocus",
                    "title": "Fix Line"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.79.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "esbuild": "^0.18.4",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@vscode/vsce": "^2.19.0",
        "axios": "^1.4.0"
    }
}
